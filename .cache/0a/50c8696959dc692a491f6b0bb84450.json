 {\n    const [location, setLocation] = useState(\"Seattle,WA\");\n    const [breeds, setBreeds] = useState([]);\n    const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\n    const [breed, BreedDropdown, setBreed] = useDropdown(\"Breed\", \"\", breeds);\n    const [pets, setPets] = useState([]);\n    const [theme,setTheme] = useContext(ThemeContext);\n\n    async function requestPets() {\n        const { animals } = await pet.animals({\n            location,\n            breed,\n            type: animal\n        });\n        setPets(animals || []);\n    }\n\n    useEffect(() => {\n        setBreeds([]);\n        setBreed(\"\");\n\n        pet.breeds(animal).then(({ breeds }) => {\n            const breedStrings = breeds.map(({ name }) => name);\n            setBreeds(breedStrings);\n        }, console.error);\n    },[animal,setBreed,setBreeds]);\n\n\n\n    return (\n        <div className=\"search-params\">\n            <h1>location</h1>\n            <form onSubmit={(e) => {\n                e.preventDefault();\n                requestPets();\n            }}>\n                <label htmlFor=\"location\">\n                    {location}\n                    <input\n                        id=\"location\"\n                        value={location}\n                        placeholder=\"Location\"\n                        onChange={e => setLocation(e.target.value)}\n                    />\n                </label>\n                <AnimalDropdown />\n                <BreedDropdown />\n                <label htmlFor=\"theme\">\n                    Theme\n                    <select\n                        value={theme}\n                        onChange={e => SVGTextPathElement(e.target.value)}\n                        onBlur={e => SVGTextPathElement(e.target.value)} />\n                    <option value=\"peru\">Peru</option>\n                    <option value=\"derkblue\">Dark Blue</option>\n                    <option value=\"chartreuse\">Chartreuse</option>\n\n                </label>\n                <button style={{color: theme}}>Submit</button>\n            </form>\n            <Results pets={pets} />\n        </div>\n    );\n};\n\nexport default SearchParams;"},"lineCount":null}},"error":null,"hash":"28511efb8be81289ca952fe00d320a36","cacheData":{"env":{}}}